{
  "name": "social-network/neo4j-api",
  "description": "Social Network API with Neo4j",
  "type": "project",
  "require": {
    "php": "^8.2",
    "laudis/neo4j-php-client": "^3.0",
    "vlucas/phpdotenv": "^5.5"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
